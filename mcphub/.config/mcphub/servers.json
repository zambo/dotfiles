{
  "mcpServers": {
    "context7": {
      "args": ["-y", "@upstash/context7-mcp"],
      "command": "npx",
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "1000"
      }
    },
    "fetch": {
      "args": ["mcp-server-fetch"],
      "command": "uvx"
    },
    "memory": {
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "command": "npx"
    },
    "sequentialthinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "command": "npx"
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": false
    },
    "neovim": {
      "disabled": false
    }
  }
}

